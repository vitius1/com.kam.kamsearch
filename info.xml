<?xml version="1.0"?>
<extension key="com.kam.kamsearch" type="module">
  <file>kamsearch</file>
  <name>Custom Search</name>
  <description>Custom search</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Vít Maňásek</author>
    <email>vmanasek@kam.cz</email>
  </maintainer>
  <releaseDate>2020-12-30</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5</ver>
  </compatibility>
  <comments>Need to add custom search on civicrm/admin/options/custom_search?reset=1 with path: CRM_Contact_Form_Search_Custom_KamSearch</comments>
